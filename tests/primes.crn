fun is_prime_a number:i5 => i0 {
    var i5 counter = number / 2
    while(counter > 1){
        if((number % counter) == 0){
            return(false)
        }
        counter -= 1 
    }
    return(true)
}

fun get_prime_after after:i5 => i5 {
    var i5 counter = after
    while(!is_prime_a(counter)){
        counter+=1
    }
    return(counter)
}

fun main => none{
    var i5 prev = 1
    while(prev < 100){
        prev = get_prime_after(prev)
        println("$n".fmt(prev))   
    }
}